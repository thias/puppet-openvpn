dev <%= @dev %>
<% if @fragment -%>
fragment <%= @fragment %>
<% end -%>
<% if @mssfix -%>
mssfix
<% end -%>
<% if @local -%>
local <%= @local %>
<% end -%>
remote <%= @remote %>
<% if @proto -%>
proto <%= @proto %>
<% end -%>
<% if @cipher -%>
cipher <%= @cipher %>
<% end -%>
<% if @float -%>
float
<% end -%>
# Local  : <%= @ipaddress_local %>
# Remote : <%= @ipaddress_remote %>
ifconfig <%= @ipaddress_local %> <%= @ipaddress_remote %>
<% if !@routes.empty? -%>
# Static routes
<% @routes.each do |route| -%>
route <%= route %>
<% end -%>
<% end -%>
secret <%= @secret %>
port <%= @port %>
<% @extra_lines.each do |line| -%>
<%= line %>
<% end -%>

<% if @comp_lzo -%>
comp-lzo
<% end -%>
user <%= @user %>
group <%= @group %>
persist-key
persist-tun
mute-replay-warnings
verb <%= @verb %>

